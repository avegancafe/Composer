{
  "manifest_version": 2,
  "name": "Composer",
  "version": "1.1.0",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "identity",
    "storage",
    "https://github.com/*",
    "https://api.github.com/*"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "minimum_chrome_version": "28"
}